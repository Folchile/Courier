<script type="text/javascript">

    $(document).ready(function(){
            CargaListaOT();
        })

    $(function(){
         $("#tabs").tabs();
//         actualizaListado();
     });

    actualizaListado();

    function actualizaListado() {
        setTimeout(function () {
           CargaListaOT();
        }, 30000);
        return false;
    }

    function CargaListaOT() {
        var randomnumber = Math.floor(Math.random() * 1111111);
        $("#ListaOTpp").html("<div style='text-align:center;'><img src='@Url.Content("~/Images/8.gif")'></div>");  
        $.ajax({
            url: '@Url.Action("_ListaOT", "Graficos")',
            data: {
                _r: randomnumber              
            }
        })
        .done(function (data) {
            $("#ListaOTpp").html(data);                
        })
        .error(function () {
            $("#ListaOTpp").html("");
            alert("No fue posible cargar la lista");            
        })
        .complete(function () {
            borrar_contenido();                        
        });
    } 




</script>


<div id="tabs">

<ul>
		
        <li><a href="#tabs-1">Detalle OT Samsung</a></li>
        
  	</ul>	
	<div id="tabs-1">
        <td style="width:100%"> <div id="ListaOTpp"></div></td>	
