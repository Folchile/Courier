@model MvcReporteViajes.Models.CabeceraFoliosModels
@{
    ViewBag.Title = "DatosVehiculo";
}

<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript")></script>

<script type="text/javascript">
    $(function () {
        $(document).ready(function () {
            borrar_contenido();
        });
    });
  

</script>


@{    
    var Texto = "";
    if (Model.MMOUSUARIO!=1) 
    {
        Texto = "disabled='disabled'";
    }
    }






<div>
@using (Ajax.BeginForm("RendicionGuardar3", "Documento", new AjaxOptions { HttpMethod = "Post", OnBegin = "javascript:Inicio()", OnSuccess = "Javascript:FinAgregar(data)", OnFailure = "Javascript:FinError()" }, new { @id = "frmAgregar3" }))
{
  <table  class="mGridFija">
       <tr>
         <td><strong>@Html.LabelFor(m=>m.VEHICULO) </strong></td>
         <td>@Html.TextBoxFor(m => m.VEHICULO, new { @style = "width:60px;font-size: 10px", @readonly = "readonly"})</td>
         <td><strong>@Html.LabelFor(m=>m.TIPO) </strong></td>
         <td>@Html.TextBoxFor(m => m.TIPO, new { @style = "width:150px;font-size: 10px", @readonly = "readonly" })</td>
         <td><strong>@Html.LabelFor(m=>m.CAPKG) </strong></td>
         <td>@Html.TextBoxFor(m => m.CAPKG, new { @style = "width:60px;font-size: 10px ;text-align:right", @readonly = "readonly" }) </td>
         <td><strong>@Html.LabelFor(m=>m.CAPM3) </strong></td>
         <td>@Html.TextBoxFor(m => m.CAPM3, new { @style = "width:60px;font-size: 10px; text-align:right", @readonly = "readonly" })</td>
         <td>@Html.TextBoxFor(m => m.ESTADO, new { @style = "width:60px;font-size: 10px;text-align:center", @readonly = "readonly" })</td>
         <td>@Html.TextBoxFor(m => m.USUARIOACTUAL, new { @style = "width:150px;font-size: 10px;text-align:left", @readonly = "readonly" })</td>
         @*<td><input type="button" @Texto id="btngrabar" name="btngrabar" value="Grabar" style="width:90px;height:25px; font-size:8pt"/></td>*@
       </tr>

  </table>
}  


</div>